linq-to-typescript\"\r\nimport {ICalculoService} from \"@services/interfaces/ICalculoService\"\r\nimport {Constants} from './../constants/constants'\r\n@injectable()\r\nclass CalculoService implements ICalculoService{\r\n\r\n    public recuperaPosicaoAtual(ordens:_model.OrdemModel[]):_model.PosicaoAtualModel[]{\r\n\r\n        let lst : _model.PosicaoAtualModel[]=[];\r\n\r\n        \r\n        const group = from(ordens).groupBy(y=>y.Papel).toArray();\r\n        \r\n        group.forEach(element=>{\r\n           let ordensTemp =  element.orderBy(y=>y.DataPregao).thenBy(y=>y.OrdemPosicao).toArray();\r\n           var posicaoAtual = this.recuperaPosicalAtualPorPapel(ordensTemp);\r\n           if (posicaoAtual != null)\r\n           {\r\n               posicaoAtual.Papel = element.key;\r\n               lst.push(posicaoAtual);\r\n           }\r\n        });\r\n\r\n        return lst;\r\n      \r\n    }\r\n\r\n    public recuperaPosicalAtualPorPapel(ordens:_model.OrdemModel[]):_model.PosicaoAtualModel | null{\r\n        let liquido : number = 0;\r\n        let  qtdEstoque :number = 0;\r\n        let  posicaoAtual: _model.PosicaoAtualModel | null = null;\r\n\r\n        for (let index = 0; index < ordens.length; index++) {\r\n            const ordemAtual = ordens[index];\r\n            \r\n            if (ordemAtual.CV == Constants.CV.COMPRA)\r\n            {\r\n                liquido += ordemAtual.Preco * ordemAtual.Quantidade * -1;\r\n                qtdEstoque += ordemAtual.Quantidade;\r\n            }\r\n            else if (ordemAtual.CV == Constants.CV.VENDA)\r\n            {\r\n                liquido += ordemAtual.Preco * ordemAtual.Quantidade;\r\n                qtdEstoque -= ordemAtual.Quantidade;\r\n\r\n            }\r\n\r\n            if (qtdEstoque == 0)\r\n            {\r\n                liquido = 0;\r\n            }\r\n        }\r\n        liquido = Math.abs(liquido);\r\n        qtdEstoque = Math.abs(qtdEstoque);\r\n\r\n        if (liquido > 0)\r\n        {\r\n            posicaoAtual =  { Valor : liquido / qtdEstoque, Quantidade : qtdEstoque, Papel :  ordens[0].Papel, ValorTotal : liquido};\r\n        }\r\n\r\n        return posicaoAtual ;\r\n    }\r\n\r\n    \r\n}\r\n\r\nexport {CalculoService};\r\n"]},"_coverageSchema":"1a1c01bbd47fc00a2c39e90264f33305004495a9","hash":"a67364745b627471709bc24432de0f686b1e8257","contentHash":"b6648b92fa1d1501d7ba7e20c1e6ac1d8b96c1489dcb5d52a69473197678ae5f"},"e:\\Programacao\\WorkSpace\\MeuDinheiro\\src\\constants\\constants.ts":{"path":"e:\\Programacao\\WorkSpace\\MeuDinheiro\\src\\constants\\constants.ts","statementMap":{"0":{"start":{"line":2,"column":0},"end":{"line":2,"column":62}},"1":{"start":{"line":3,"column":0},"end":{"line":3,"column":27}},"2":{"start":{"line":5,"column":0},"end":{"line":23,"column":62}},"3":{"start":{"line":6,"column":33},"end":{"line":13,"column":7}},"4":{"start":{"line":9,"column":8},"end":{"line":9,"column":80}},"5":{"start":{"line":10,"column":8},"end":{"line":10,"column":95}},"6":{"start":{"line":11,"column":8},"end":{"line":11,"column":73}},"7":{"start":{"line":12,"column":8},"end":{"line":12,"column":23}},"8":{"start":{"line":14,"column":4},"end":{"line":14,"column":32}},"9":{"start":{"line":15,"column":28},"end":{"line":21,"column":7}},"10":{"start":{"line":18,"column":8},"end":{"line":18,"column":24}},"11":{"start":{"line":19,"column":8},"end":{"line":19,"column":23}},"12":{"start":{"line":20,"column":8},"end":{"line":20,"column":18}},"13":{"start":{"line":22,"column":4},"end":{"line":22,"column":22}}},"fnMap":{"0":{"name":"(anonymous_0)","decl":{"start":{"line":5,"column":1},"end":{"line":5,"column":2}},"loc":{"start":{"line":5,"column":22},"end":{"line":23,"column":1}},"line":5},"1":{"name":"(anonymous_1)","decl":{"start":{"line":6,"column":33},"end":{"line":6,"column":34}},"loc":{"start":{"line":6,"column":45},"end":{"line":13,"column":5}},"line":6},"2":{"name":"Arquivo","decl":{"start":{"line":7,"column":17},"end":{"line":7,"column":24}},"lo